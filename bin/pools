#!/usr/bin/env php
<?php

require __DIR__ . '/../vendor/autoload.php';

use PoolsPhp\Pools\Commands\InstallRectorCommand;
use Symfony\Component\Console\Application;

$application = new Application();

$application->add(new InstallRectorCommand());

try {
    $application->run();
} catch (Exception $e) {
    echo $e->getMessage();
    exit(1);
}
