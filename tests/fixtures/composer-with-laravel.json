{
    "name": "pools-php/pools",
    "description": "A simple",
    "keywords": ["php", "tools", "package"],
    "license": "MIT",
    "authors": [
        {
            "name": "Francisco Barrento",
            "email": "francisco.barrento@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3.0",
        "laravel/framework": "^12.0",
        "symfony/console": "^7.2",
        "symfony/process": "^7.2"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "PoolsPhp\\Pools\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
